{
  "name": "assignment-manager",
  "version": "1.0.0",
  "description": "Discord Task Management Bot with Clean Architecture",
  "main": "src/main.ts",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node dist/main.js",
    "dev": "tsx src/main.ts",
    "deploy-commands": "tsx src/presentation/deploy-commands.ts",
    "invite-bot": "tsx src/scripts/generate-invite.ts",
    "db:migrate": "prisma migrate dev",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write src/**/*.ts",
    "format:check": "prettier --check src/**/*.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nomanoma121/assignment-manager.git"
  },
  "keywords": [
    "discord",
    "bot",
    "task-management",
    "typescript"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^24.0.3",
    "@types/node-cron": "^3.0.11",
    "@typescript-eslint/eslint-plugin": "^8.17.0",
    "@typescript-eslint/parser": "^8.17.0",
    "@vitest/ui": "^2.1.8",
    "eslint": "^9.18.0",
    "prettier": "^3.4.2",
    "prisma": "^6.1.0",
    "tsx": "^4.19.2",
    "typescript": "^5.8.3",
    "vitest": "^2.1.8"
  },
  "dependencies": {
    "@discordjs/builders": "^1.9.0",
    "@prisma/client": "^6.1.0",
    "discord.js": "^14.19.3",
    "dotenv": "^16.5.0",
    "node-cron": "^3.0.3",
    "reflect-metadata": "^0.2.2",
    "tsyringe": "^4.8.0"
  }
}
